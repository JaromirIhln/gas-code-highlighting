{
  "name": "GAS Light Colors",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#333333"
  },
  "tokenColors": [
    {
      "scope": [
        "variable.gas"
      ],
      "settings": {
        "foreground": "#0070C0" // modr치 pro variable
      }
    },
    {
      "scope": [
        "identifier.gas"
      ],
      "settings": {
        "foreground": "#228B22", // zelen치 pro identifier,
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.quoted.double.gas, string.quoted.single.gas",
      "settings": {
        "foreground": "#b75501"
      }
    },
    {
      "scope": "comment.line.double-slash.gas, comment.block.gas",
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric.gas",
      "settings": {
        "foreground": "#6abded"
      }
    },
    {
      "scope": "comment.documentation.gas",
      "settings": {
        "foreground": "#b8860b", // zlat치, nebo jin치 barva dle preference
        "fontStyle": "italic"
      }
    }
  ]
}