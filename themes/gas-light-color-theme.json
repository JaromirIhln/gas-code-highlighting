{
  "name": "GAS Light Colors",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": { "foreground": "#4acd62", "fontStyle": "italic" }
    },
    {
      "scope": "string",
      "settings": { "foreground": "#b3883f" }
    },
    {
      "scope": "constant.numeric", 
      "settings": { "foreground": "#e3aae7" }
    },
    {
      "scope": "keyword",
      "settings": { "foreground": "#60b5fb", "fontStyle": "bold" }
    },
    {
      "scope": "variable.other.gas",
      "settings": { "foreground": "#4e59fd" }
    },
    {
      "scope": "entity.name.function.gas", 
      "settings": { "foreground": "#efb962", "fontStyle": "bold" }
    },
    {
      "scope": "support.class.gas",
      "settings": { "foreground": "#4EC9B0", "fontStyle": "bold" }
    }
  ]
}
