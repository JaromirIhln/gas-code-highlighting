{
  "name": "GAS Simple Colors",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4"
  },
  "tokenColors": [
    {
      "scope": "keyword.control.gas",
      "settings": { "foreground": "#FF5F00", "fontStyle": "bold" }
    },
    {
      "scope": "string.quoted.double.gas",
      "settings": { "foreground": "#00BFFF" }
    },
    {
      "scope": "string.quoted.single.gas",
      "settings": { "foreground": "#00BFFF" }
    },
    {
      "scope": "comment.line.double-slash.gas",
      "settings": { "foreground": "#888888", "fontStyle": "italic" }
    },
    {
      "scope": "comment.block.gas",
      "settings": { "foreground": "#888888", "fontStyle": "italic" }
    },
    {
      "scope": "comment.documentation.gas",
      "settings": { "foreground": "#b8860b", "fontStyle": "italic" }
    },
    {
      "scope": "constant.numeric.gas",
      "settings": { "foreground": "#FFD700" }
    },
    {
      "scope": "variable.other.gas",
      "settings": { "foreground": "#00FF00" }
    }
  ]
}
